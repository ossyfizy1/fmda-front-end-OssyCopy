  <body class="vertical-layout vertical-content-menu material-vertical-layout material-layout 1-column  bg-full-screen-image" data-open="click" data-menu="vertical-content-menu" data-col="1-column">
    <!-- BEGIN: Content-->
    <div class="app-content content" style="margin-top: 10px">
      <div class="content-header row">
      </div>
      <div class="content-wrapper">
        <div class="content-body"><section class="flexbox-container">
    <div class="col-12 d-flex align-items-center justify-content-center">
        <div class="col-lg-12 col-md-8 col-10 box-shadow-2 p-0">
            <div class="card border-grey border-lighten-3 px-1 py-1 m-0">
                <div class="card-header border-0 pb-0">
                    <div class="card-title text-center">
                        <img src="app-assets/images/logo/register.png" alt="branding logo">
                    </div>
                </div>
                <div class="card-content">

                    <p class="card-subtitle line-on-side text-muted text-center font-small-3 mx-2 my-1"><span>Finalize Individual Migration Registration Process</span></p>
                    <div class="card-body">


                    <div class="col-md-12">

                        <form class="form-horizontal" action="/ind_concludeRegistrationFromBulkUpload" method="POST" enctype="multipart/form-data">
                            

                            <div class="row">
                                <div class="col-md-12 form-group position-relative">
                                    <label for="newPassword">Email</label>
                                    <input type="text" name="email" value="<%=email%>" required readonly class="form-control password border-primary">
                                </div>
                                <div class="col-md-6 form-group position-relative">
                                    <label for="newPassword">New Password</label>
                                    <input type="password" name="newPassword" id="newPassword" required class="form-control password border-primary" placeholder="Enter new password" name="newPassword">
                                </div>
                                <fieldset class="col-md-6 form-group position-relative">
                                    <label for="confirmPassword">Confirm Password</label>
                                    <input type="password" name="confirmPassword" id="confirmPassword" required class="form-control password border-primary" onblur="comparePassword()" placeholder="Confirm password">
                                </fieldset>
                            </div>

                            <hr>

                            <div class="row">
                                <hr>
                                <div class="col-md-6 form-group position-relative">
                                    <select name="questionID" class="form-control">
                                        <option value="0">Security Questions</option>
                                        <% 
                                            var securityQuestions = securityQuestionsGotten;
                                            var arrayLength = securityQuestions.length;
                                            for(i=0; i < arrayLength; i++){
                                        %>
                                            <option value="<%= securityQuestions[i].id %>"> <%= securityQuestions[i].question %> </option>
                                        <%
                                        }
                                        %>
                                    </select>
                                </div>
                                <div class="col-md-6 form-group position-relative">
                                    <input name="answer" type="password" class="form-control" id="user-password" placeholder="Provide Answer" required>
                                </div>
                            </div>

                            <hr>

                            <div class="row">
                                <div class="col-md-6 form-group position-relative">
                                    <label for="firstName1">Passport Upload (JPG, PNG, JPEG) :</label><br>
                                    <input name="treasuryCertification" type="file" id="profilePhoto" placeholder="profile photo" required> 
                                </div>

                                <div class="col-md-6 form-group position-relative">
                                    <label style="margin-top: 20px;">Comprehensive CV (pdf)</label>
                                    <input name="treasuryCertification" type="file" class="form-control" id="treasuryCertification" placeholder="Treasury Certification" required>
                                </div>
                            </div>

                            <hr>
                            
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label>Select Your Certification Type(s) to upload (minimum type & upload of 1):</label>
                                            <hr>
                                        </div>

                                    <% 
                                        var tType = treasuryCertificateType.treasuryCertificationTypes;
                                        for(i = 0; i < tType.length; i++)
                                            {
                                                if( tType[i].name != "Others"){
                                    %>
                                        <div class="col-md-3">
                                            <input type="checkbox" name="certificationType" id="certificationType" value="<%= tType[i].name %>" /> <%= tType[i].name %>    
                                        </div>
                                    <% 
                                                }       
                                            } 
                                    %> 
                                    </div>

                                    <hr>

                                </div>


                                <div class="col-md-3 form-group position-relative">
                                    <label> Certification Type 1 (pdf)</label>
                                    <input name="treasuryCertification" type="file" class="form-control" id="treasuryCertification" placeholder="Treasury Certification" required>    
                                </div>

                                <div class="col-md-3 form-group position-relative">
                                    <label>Certification Type 2 (pdf)</label>
                                    <input name="treasuryCertification" type="file" class="form-control" id="treasuryCertification" placeholder="Treasury Certification">    
                                </div>

                                <div class="col-md-3 form-group position-relative">
                                    <label>Certification Type 3 (pdf)</label>
                                    <input name="treasuryCertification" type="file" class="form-control" id="treasuryCertification" placeholder="Treasury Certification">    
                                </div> 

                            </div>

                            <hr>



                            <div class="row">
                                <div class="col-md-12">
                                    <label>Provide Other Qualification (minimum of 1):</label>
                                    <hr>
                                </div>

                                <div class="col-md-3 form-group position-relative">
                                    <label for="newPassword">Other Qualification</label>
                                    <input type="file" name="treasuryCertification" required />
                                </div>
                                <div class="col-md-3 form-group position-relative">
                                    <label for="phoneNumber1"> Institution Name :</label>
                                    <input type="text" class="form-control" name="institutionName" placeholder="Institution Name" required>
                                </div>
                                <div class="col-md-3 form-group position-relative">
                                    <label for="phoneNumber1"> Qualification :</label>
                                    <input type="text" class="form-control" name="qualificationType" placeholder="Qualification Type" required>
                                </div>
                                <div class="col-md-3 form-group position-relative">
                                    <label>Year Attained :</label>
                                    <input type='date' class="form-control" name="yearAttained" required/>
                                </div>
                            </div>

                            <hr>

                            <div class="row">
                                <div class="col-md-3 form-group position-relative">
                                    <label for="newPassword">Other Qualification</label>
                                    <input type="file" name="treasuryCertification" />
                                </div>
                                <div class="col-md-3 form-group position-relative">
                                    <label for="phoneNumber1"> Institution Name :</label>
                                    <input type="text" class="form-control" name="institutionName" placeholder="Institution Name">
                                </div>
                                <div class="col-md-3 form-group position-relative">
                                    <label for="phoneNumber1"> Qualification :</label>
                                    <input type="text" class="form-control" name="qualificationType" placeholder="Qualification Type">
                                </div>
                                <div class="col-md-3 form-group position-relative">
                                    <label>Year Attained :</label>
                                    <input type='date' class="form-control" name="yearAttained" />
                                </div>
                            </div>

                            <hr>

                            

                                
                            <div class="col-md-3 form-group position-relative">
                                <!-- Button trigger modal -->
                                <input type="submit" name="submit" value="submit" class="btn btn-light block btn-lg" />
                            </div>
                        </form>


                        <script>
                            const showPassword = () => {
                                const passwordFields = document.querySelectorAll('.password');
                                passwordFields.forEach(item => {
                                    if (item.type === 'password') item.type = 'text'
                                    else item.type = 'password';
                                });
                            }

                            const comparePassword = () => {
                                const password = document.querySelector('#newPassword').value;
                                const confirmPassword = document.querySelector('#confirmPassword').value;
                                const submitBtn = document.querySelector('#submitBtn');
                                if (password !== confirmPassword) {
                                    alert('password and confirm password do not match');
                                    submitBtn.disabled = true;
                                } else submitBtn.disabled = false;
                            }
                        </script>




                    </div>

                        

                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

        </div>
      </div>
    </div>
