                            <!-- to show password -->
                            <script>
                                function showPassword() {
                                    var x = document.getElementById("myInput");
                                    var y = document.getElementById("myInput2");

                                    if (x.type === "password" && y.type === "password") {
                                        x.type = "text";
                                        y.type = "text";
                                    } else {
                                        x.type = "password";
                                        y.type = "password";
                                    }
                                }
                            </script>


                            <!-- compare password and confirm password supplied -->
                            <script>
                                function comparePassword() {
                                    // get the value the first password
                                    var myInput = $("#myInput").val();
                                        // console.log(myInput);

