                    <p class="card-subtitle line-on-side text-muted text-center font-small-3 mx-2 my-1"><span>Register</span></p>
                    <div class="card-body">

                        <form action="/register_individual" method="post" class="form-horizontal">
                            <fieldset class="form-group position-relative">
                                <input name="firstName" type="text" class="form-control" id="firstName" placeholder="First Name" required>
                                <div class="form-control-position">
                                    <i ></i>
                                </div>
                            </fieldset>
                            <fieldset class="form-group position-relative">
                                <input  name="lastName" type="text" class="form-control" id="user-name" placeholder="Last Name" required>
                                <div class="form-control-position">
                                    <i></i>
                                </div>
                            </fieldset>
                            <fieldset class="form-group position-relative">
                                <input name="personalEmail" type="email" class="form-control" id="user-email" placeholder="Personal Email" required>
                                <div class="form-control-position">
                                    <i ></i>
                                </div>
                            </fieldset>
                            <fieldset class="form-group position-relative">
                                <input name="mobileNumber" type="phone" class="form-control" id="telephone" placeholder="Mobile Number" required>
                                <div class="form-control-position">
                                    <i></i>
                                </div>
                            </fieldset>
                            <fieldset class="form-group position-relative">
                                <input name="password" type="password" class="form-control" id="myInput" placeholder="Enter Password" required>
                                <div class="form-control-position">
                                    <i ></i>
                                </div>
                            </fieldset>
                            <fieldset class="form-group position-relative">
                                <input name="confirmpassword" type="password" class="form-control" id="myInput2" placeholder="Confirm Password" required onblur="comparePassword()">

                                <br>
                                <input type="checkbox" onclick="showPassword()"> Show Password
                                <div class="form-control-position">
                                    <i ></i>
                                </div>
                            </fieldset>
                            
                            
                            <div class="form-group position-relative">
                                    <!-- Button trigger modal -->
                                    <button type="submit" id="buttonIDToUse" class="btn btn-light block btn-lg" data-toggle="modal" data-target="#inlineForm">
                                        Register
                                    </button>

                                
                                </div>
                            </div>



