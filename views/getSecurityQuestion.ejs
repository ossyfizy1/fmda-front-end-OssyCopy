    <!-- BEGIN: Content-->
    <div class="app-content content">
      <div class="content-header row">
      </div>
      <div class="content-wrapper">
        <div class="content-body"><section class="flexbox-container">
    <div class="col-12 d-flex align-items-center justify-content-center">
        <div class="col-lg-4 col-md-8 col-10 box-shadow-2 p-0">
            <div class="card border-grey border-lighten-3 px-1 py-1 m-0">
                <div class="card-header border-0">
                    <div class="card-title text-center">
                        <img src="app-assets/images/logo/login.png" alt="branding logo">
                    </div>
                </div>
                <div class="card-content">

                    <p class="card-subtitle line-on-side text-muted text-center font-small-3 mx-2 my-1"><span>Provide Security Answer To Question</span></p>
                    <div class="card-body">
                        <form action="/validateSecurityAnswer" method="POST" class="form-horizontal" novalidate>
                            <fieldset class="form-group position-relative has-icon-left">
                                <input name="userID" type="text" class="form-control" id="user-name" value="<%= userID %>" readonly>
                                <div class="form-control-position">
                                    <i ></i>
                                </div>
                            </fieldset>
                            <fieldset class="form-group position-relative has-icon-left">
                                <input name="securityQuestion" type="text" class="form-control" id="user-name" value="<%= securityQuestionGotten.question %>" readonly>
                                <div class="form-control-position">
                                    <i ></i>
                                </div>
                            </fieldset>
                            <fieldset class="form-group position-relative has-icon-left">
                                <input name="answer" type="password" class="form-control" id="myInput" placeholder="Enter Answer" required>
                                <input type="checkbox" onclick="showPassword()"> Show Password
                                <div class="form-control-position">
                                    <i ></i>
                                </div>
                            </fieldset>

                            <!-- to show password -->
                            <script>
                                function showPassword() {
                                    var x = document.getElementById("myInput");
                                    if (x.type === "password") {
                                    x.type = "text";
                                    } else {
                                    x.type = "password";
                                    }
                                }
                            </script>

                            <button type="submit" class="btn btn-outline-dark btn-block">Verify</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

